# Alpaca
ALPACA_KEY_ID=YOUR_KEY
ALPACA_SECRET_KEY=YOUR_SECRET
ALPACA_PAPER=true
ALPACA_DATA_FEED=sip   # or iex

# S3
S3_BUCKET=your-bucket-name
S3_REGION=us-east-1
S3_BASE_PREFIX=models

# Universe (hub default)
SYMBOLS=SPY,QQQ,AAPL,MSFT,NVDA

# Trading guardrails
START_EQUITY=100000
DAILY_PROFIT_TARGET_PCT=0.10
DAILY_DRAWDOWN_PCT=0.05
GUARD_FLATTEN=true
EOD_FLATTEN_START_ET=16:00
EOD_FLATTEN_END_ET=16:10

# Rate limits (per minute)
ORDER_RATE_LIMIT_PM=120
STATUS_RATE_LIMIT_PM=240

# Strategies (comma-separated)
STRATEGY_NAMES=ranked_ml,mean_reversion

# Strategy overrides (no code edits)
S_RANKED_ML_CONF_THR=0.7
S_RANKED_ML_SYMBOLS=SPY,QQQ,MSFT,NVDA,META,AMZN
S_RANKED_ML_MAX_POSITIONS=200
S_RANKED_ML_MIN_PRICE=3.0
S_RANKED_ML_SHORTS_ENABLED=false

S_MEAN_REV_CONF_THR=0.6
S_MEAN_REV_SYMBOLS=AAPL,AMD,TSLA
S_MEAN_REV_MAX_POSITIONS=50
S_MEAN_REV_MIN_PRICE=3.0
S_MEAN_REV_SHORTS_ENABLED=false
